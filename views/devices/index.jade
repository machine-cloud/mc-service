extends ../layout

block script

  script(src="/js/devices.js")

block content

  each model in models
    .container.model(id="model-#{model.name}", data-name=model.name)
      h3= model.name
      h4= model.description

      table.table.table-striped.table-bordered#devices
        thead
          tr
            th ID
            th Last Message
        tbody
